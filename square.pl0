//variable which will be squared
var x;
//infinite loop
while 0=0 do begin
	//read variable
	read x;
	//square variable
	//testing another operations too, result after following
	//two lines should be only x*x
	x:=not ((0xf xor ((x*x left 3) right 2)/2) xor 0xf);
	x:=not ((x xor x) or (x and x));
	//write variable
	write x
end.
